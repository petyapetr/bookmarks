{
	"name": "bookmarks-saratov",
	"version": "0.1.0",
	"description": "Personal Bookmarks Lib",
	"type": "module",
	"scripts": {
		"dev:uno": "pnpm exec unocss --watch 'src/templates/**/*.html' -o 'public/uno.css' -c 'uno.config.js'",
		"dev:node": "node --watch --env-file=.env src/server.js",
		"dev": "pnpm run \"/dev:/\"",
		"start": "node --env-file=.env.prod src/server.js"
	},
	"keywords": [
		"h3",
		"sqlite",
		"unjs"
	],
	"author": "Slava Zakharov",
	"license": "0BSD",
	"packageManager": "pnpm@10.18.3",
	"dependencies": {
		"better-sqlite3": "^12.4.1",
		"h3": "2.0.1-rc.5",
		"nunjucks": "^3.2.4"
	},
	"devDependencies": {
		"@types/node": "^24.9.2",
		"@unocss/cli": "^66.5.5",
		"@unocss/preset-mini": "^66.5.5",
		"prettier": "3.6.2",
		"prettier-plugin-jinja-template": "^2.1.0",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"unocss": "^66.5.5"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"better-sqlite3"
		]
	}
}
